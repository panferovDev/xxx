
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Group {
  id Int @id @default(autoincrement())
  name String
  students Student[]
}

model Student {
  id Int @id @default(autoincrement())
  name String
  git String
  groupId Int
  group Group @relation(fields: [groupId], references: [id])
}